{
  "name": "saki-control-plane",
  "version": "0.1.0",
  "private": true,
  "description": "Express.js control plane scaffold for Saki",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "docker:build": "docker build -t saki-control-plane:local .",
    "docker:build:push": "bash scripts/build-and-push.sh"
  },
  "dependencies": {
    "@kubernetes/client-node": "^1.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "helmet": "^8.1.0",
    "morgan": "^1.10.0",
    "pg": "^8.13.3",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.23",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.15.2",
    "@types/pg": "^8.16.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3"
  }
}
